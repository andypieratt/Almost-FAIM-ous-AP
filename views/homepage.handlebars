<div class="container browser-frame center">
    <div id="drag-handle"></div>
    <main class="content">
        <div class="couldnt-do-this-in-the-90s">
            <h2>Welcome to fAIM!</h2>
            <h3>LOGIN</h3>
        </div>
        <p>Throwback 90's style instant messaging!</p>

        <form id="login-form">
            <!-- Email input -->
            <div>
                <input id="username" class="form-group-input" type="" /><br>
                <label class="form-label" for="form2Example1">Username</label>
            </div>

            <!-- Password input -->
            <div class="form-group-input">
                <input type="password" id="password" class="form-control" /><br>
                <label class="form-label" for="form2Example2">Password</label>
            </div>

            <!-- Submit button -->
            <button type="submit" id="submit-button" class="btn btn-info">Sign in</button>
            <div class="text-center">

            </div>
            <!-- Register buttons -->
            <div class="text-center">
                <p>Not a member? <button type="button" class="btn btn-info"><a href="/register">Register</a> </button></p>
            </div>
        </form>
 
    </main>
   
</div>
<img class="footer" src="/images/start-bar.png" alt="start-bar">
